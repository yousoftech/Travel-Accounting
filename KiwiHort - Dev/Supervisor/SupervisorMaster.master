    <style type="text/css">
        div#sticky-wrapper {
            background: #00a65a;
        }

        .skin-blue .main-header .navbar .nav > li > a {
            font-size: 18px;
        }

        ol.breadcrumb {
            font-size: 15px !important;
        }

        ul#main-menu-navigation ul.dropdown-menu li {
            width: auto;
            text-align: left;
        }

        ul#main-menu-navigation li {
            width: 112px;
            text-align: center;
            float: left;
        }

        ul#main-menu-navigation a i {
            display: block;
            font-size: 30px;
        }

        a.dropdown-toggle.nav-link {
            color: #fff;
        }

            a.dropdown-toggle.nav-link img {
                width: 40px;
                display: block;
                margin: 0 auto;
            }

        @media (max-width:768px) {
            .dev img {
                width: 100% !important;
                height: 100% !important;
            }

            ul#main-menu-navigation li {
                width: 80px;
            }

            ul#main-menu-navigation {
                margin-left: -10px;
            }
        }

        @media (max-width:320px) {
            .nav > li > a {
                position: relative;
                display: block;
                padding: 10px 9px;
            }

            ul#main-menu-navigation li {
                width: 65px;
                margin-right: 10px;
            }

                ul#main-menu-navigation li:last-child a {
                    min-width: 101px !important;
                }
        }

        .content {
            min-height: 440px !important;
        }

        .btn-warning {
            background-color: #f39c12;
            border-color: #f39c12;
        }

        .navbar-toggle .icon-bar {
            background: #00a65a;
        }

        a.logo img {
            height: 50px;
        }

        #mob-menu {
            display: none;
        }

        @media only screen and (max-width: 768px) {
            div#sticky-wrapper {
                display: none;
            }

            #mob-menu {
                display: block;
            }

            div#bs-example-navbar-collapse-1 {
                overflow-y: initial;
            }

            ul#mob-menu {
                width: 100%;
                background: #00a65a;
                margin: 50px 0;
                border-radius: 10px 0 10px 10px;
            }

                ul#mob-menu li a {
                    line-height: 40px;
                    padding: 7px 10px;
                }

                    ul#mob-menu li a img {
                        float: left;
                        margin-right: 5px;
                    }
        }
    </style>
